export ANT_HOME=/usr/local/opt/apache-ant-1.9.15
export MVN_HOME=/usr/local/opt/apache-maven-3.6.3
export ANT_OPTS='-Xms1024M -Xmx4096M -XX:MaxPermSize=512m'
export IVY_SERVER=http://ivyrot.sfdp.c.eu-de-2.cloud.sap:8081
export MAVEN_OPTS='-Xmx3000m'
export SFS_BIND_PORT_SHUTDOWN=8006
export ICU4V_HOME=/usr/local/opt/icu4c/
export MYSQL_HOME=/usr/local/mysql
export LDFLAGS="-L/usr/local/opt/tcl-tk/lib"
export CPPFLAGS="-I/usr/local/opt/tcl-tk/include"
export PYTHON3="/Users/i312177/opt/anaconda3"
export ZOOKEEPER_HOME="/usr/local/opt/apache-zookeeper-3.6.2-bin"
export JMETER_HOME="/usr/local/opt/apache-jmeter-5.4"
export SMERGE_HOME=/Users/i312177/software/Sublime\ Merge.app/Contents/SharedSupport/bin
